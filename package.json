{
  "name": "kof-slide",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:kof": "slidev slides/kof/slides.md --open",
    "dev:red": "slidev slides/red/slides.md --open",
    "build:kof": "slidev build slides/kof/slides.md --base /kof/ --out ../../dist/kof",
    "build:red": "slidev build slides/red/slides.md --base /red/ --out ../../dist/red",
    "build": "npm run build:kof && npm run build:red"
  },
  "dependencies": {
    "@slidev/cli": "^52.1.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "playwright-chromium": "^1.55.0"
  }
}